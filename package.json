    "html2plaintext": "^2.1.2",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "shuffle-array": "^1.0.1",
    "string-strip-html": "^4.4.7",
    "telegraf": "^3.37.0",
    "telegraf-session-local": "^1.0.2",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "ava": "^3.8.2",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
